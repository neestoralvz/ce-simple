{
  "environment": "production",
  "description": "Production environment configuration optimized for performance and reliability",
  
  "agent_overrides": {
    "all_agents": {
      "debug_mode": false,
      "response_validation": "standard",
      "intermediate_logging": false,
      "verbose_output": false,
      "safety_checks": "standard"
    },
    "planning_agent": {
      "token_limit": 7000,
      "timeout_seconds": 300
    },
    "validation_agent": {
      "max_parallel_tasks": 8,
      "error_tolerance": "none"
    }
  },
  
  "coordination_overrides": {
    "max_concurrent_agents": 15,
    "error_retry_limit": 3,
    "timeout_multiplier": 1.0,
    "checkpoint_frequency": "per_phase",
    "detailed_logging": false
  },
  
  "monitoring": {
    "performance_tracking": true,
    "resource_monitoring": false,
    "coordination_analytics": false,
    "execution_tracing": false,
    "memory_profiling": false,
    "tool_usage_stats": true
  },
  
  "optimization": {
    "aggressive_caching": true,
    "result_compression": true,
    "early_termination": true,
    "resource_pooling": true,
    "batch_operations": true
  },
  
  "reliability": {
    "automatic_failover": true,
    "circuit_breaker": true,
    "health_checks": true,
    "graceful_degradation": true,
    "rollback_on_failure": true
  },
  
  "security": {
    "validate_all_inputs": true,
    "sanitize_outputs": true,
    "limit_file_access": true,
    "audit_all_changes": true
  }
}