{
  "environment": "development",
  "description": "Development environment configuration with enhanced debugging and monitoring",
  
  "agent_overrides": {
    "all_agents": {
      "debug_mode": true,
      "response_validation": "strict",
      "intermediate_logging": true,
      "verbose_output": true,
      "safety_checks": "enhanced"
    },
    "discovery_agent": {
      "token_limit": 5000,
      "timeout_seconds": 450
    },
    "execution_agent": {
      "max_parallel_tasks": 6,
      "error_tolerance": "medium"
    }
  },
  
  "coordination_overrides": {
    "max_concurrent_agents": 10,
    "error_retry_limit": 5,
    "timeout_multiplier": 2.0,
    "checkpoint_frequency": "per_task",
    "detailed_logging": true
  },
  
  "monitoring": {
    "performance_tracking": true,
    "resource_monitoring": true,
    "coordination_analytics": true,
    "execution_tracing": true,
    "memory_profiling": true,
    "tool_usage_stats": true
  },
  
  "debugging": {
    "break_on_errors": true,
    "save_intermediate_results": true,
    "log_all_tool_calls": true,
    "preserve_temp_files": true,
    "detailed_error_messages": true
  },
  
  "safety": {
    "file_backup_before_edit": true,
    "validate_before_execution": true,
    "limit_destructive_operations": true,
    "require_confirmation": ["file_deletion", "major_changes"]
  }
}