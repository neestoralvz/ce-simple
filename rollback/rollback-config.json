{
  "rollback_version": "1.0.0",
  "created": "$(date -Iseconds)",
  "project_root": "$(pwd)",
  "critical_files": [
    "CLAUDE.md",
    "README.md",
    ".gitignore",
    ".session-info",
    "commands/",
    "docs/core/",
    "docs/standards/"
  ],
  "validation_thresholds": {
    "critical_errors": 0,
    "high_errors": 2,
    "medium_errors": 5,
    "performance_degradation_percent": 20
  },
  "backup_retention_days": 30,
  "auto_rollback_enabled": true
}
